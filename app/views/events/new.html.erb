<% provide(:title, '新規登録') %>
<h1>イベント新規登録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@event) do |f| %>
      <%#= render 'shared/error_messages', object: f.object %>
      <%= f.label :イベント名 %>
      <%= f.text_field :title, class: 'form-control' %>
      <%= f.label :場所 %>
      <%= f.email_field :place, class: 'form-control' %>
      <%= f.label :場所リンク %>
      <%= f.text_field :placelink, class: 'form-control' %>
      <br>
      <%= f.submit "新規登録", class: "btn btn-primary" %>
    <% end %>
    <p><center>
     <A HREF="javascript:history.back()">前のページに戻る</A>
    </center></p>
  </div>
</div>